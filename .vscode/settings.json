{
  "i18n-ally.localesPaths": ["frontend-react/public/locales"],
  "i18n-ally.keystyle": "nested",
  "i18n-ally.sortKeys": true,
  "i18n-ally.enabledFrameworks": ["next-translate", "react"],
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "i18n-ally.sourceLanguage": "es"
}
